{
  "name": "simpel",
  "displayName": "component templater",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:simpel.applyTemplate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "simpel.applyTemplate",
        "title": "Apply Template",
        "category": "simpel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@hediet/node-reload": "^0.7.3",
    "@hediet/std": "^0.6.0",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.9",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "change-case": "^4.1.2",
    "eslint": "^8.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "handlebars": "^4.7.7",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "xo": "^0.52.4"
  }
}